{
  "name": "uni-project-template",
  "private": true,
  "scripts": {
    "docker:start": "docker compose up -d && docker compose exec drupal bash",
    "docker:clean": "docker compose down --rmi local",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:logs": "pm2 logs --raw | pino-pretty"
  },
  "devDependencies": {
    "pino-pretty": "^9.1.1"
  },
  "dependencies": {
    "github-webhooks-exec": "^1.3.1"
  }
}
